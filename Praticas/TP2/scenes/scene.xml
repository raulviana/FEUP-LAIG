        </primitive>
        <primitive id="Cylinder">
            <cylinder top="1" base="1"  height="3" stacks="10" slices="10" />
        </primitive>
        <primitive id="Sphere">
            <sphere radius="0.5" slices="100" stacks="100"/>
        </primitive>
        <primitive id="Torus">
            <torus inner="0.5" outer="1" slices="100" loops="100" />
        </primitive>
        <primitive id="Plane">
            <plane npartsU="5" npartsV="5" />
        </primitive>
        <primitive id="Patch">
            <patch npointsU="3" npointsV="2" npartsU="20" npartsV="20" >
                <controlpoint xx="-1.5" yy="-1.5" zz="0.0" />
                <controlpoint xx="-1.5" yy="1.5" zz="0.0" />
                <controlpoint xx="0.0" yy="-1.5" zz="3.0" />
                <controlpoint xx="0.0" yy="1.5" zz="3.0" />
                <controlpoint xx="1.5" yy="-1.5" zz="0.0" />
                <controlpoint xx="1.5" yy="1.5" zz="0.0" />
            </patch>
        </primitive>
        <primitive id="Cylinder2">
