<lxs>
	<scene root="root" axis_length="5" />
	<views default="defaultCamera">
		<perspective id="defaultCamera" near="0.1" far="500" angle="45">
			<from x="30" y="15" z="30" />
			<to x="0" y="-2" z="0" />
		</perspective>
	</views>
	<ambient>
		<ambient r="0.2" g="0.2" b="0.2" a="1.0" />
		<background r="0.0" g="0.0" b="0.0" a="1.0" />
	</ambient>
	<lights>
		<omni id="demoLight" enabled="1">
			<location x="9.8" y="11.9" z="10" w="1.0" />
			<ambient r="0" g="0" b="0" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
		</omni>
	</lights>
	<textures>
		<texture id="demoTexture" file="scenes/images/aefeup.jpg" />
	</textures>
	<materials>
		<material id="demoMaterial" shininess="10">
			<emission r="0.0" g="0.0" b="0.0" a="1.0" />
			<ambient r="1.0" g="0.0" b="0.0" a="1.0" />
			<diffuse r="0.6" g="0.0" b="0.0" a="1.0" />
			<specular r="0.6" g="0.0" b="0.0" a="1.0" />
		</material>
	</materials>
	<transformations>
		<transformation id="demoTransform">
			<scale x="5" y="1" z="5" />
			<rotate axis="x" angle="-45" />
			<rotate axis="z" angle="-45" />
		</transformation>
	</transformations>
	<primitives>
		<primitive id="bladeRect">
			<rectangle x1="0" y1="0" x2="1" y2=".4" />
		</primitive>
		<primitive id="sphere">
			<sphere radius="1.7" slices="12" stacks="8" />
		</primitive>
		<primitive id="cylinder8">
			<cylinder base="0.3" top="0.3" height="2.5" slices="8" stacks="1" />
		</primitive>
		<primitive id="pyramid8">
			<cylinder base="0.3" top="0" height="0.9" slices="8" stacks="1" />
		</primitive>
		<primitive id="torus">
			<torus inner=".2" outer="1.2" slices="12" loops="24" />
		</primitive>
		<primitive id="cylinder3">
			<cylinder base="0.3" top="0.3" height="4" slices="3" stacks="1" />
		</primitive>
	</primitives>
	<components>
		<component id="root">
			<transformation></transformation>
			<materials>
				<material id="demoMaterial" />
			</materials>
			<texture id="demoTexture" />
			<children>
				<componentref id= "body" />
				<componentref id="arm1" />
				<componentref id="arm2" />
				<componentref id="arm3" />
				<componentref id="arm4" />
			</children>
		</component>
		<component id="body">
			<transformation>
				<scale x="1" y="0.6" z="1" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<primitiveref id= "sphere" />
			</children>
		</component>
		<component id="arm">
			<transformation></transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="blades" />
				<componentref id= "blade_suport" />
			</children>
		</component>
		<component id="blade_protection">
			<transformation>
				<scale x="1" y="2.1" z="1" />
				<rotate axis="x" angle="90"/>
			</transformation>
			<materials >
				<material id="inherit" />
			</materials>
			<texture id="none" />
			<children>
				<primitiveref id="torus" />
			</children>
		</component>
		<component id="blades">
			<transformation>
				<translate x="4" y="1.5" z="0" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="blade_protection" />
				<componentref id= "blade_motor" />
				<componentref id="blade_1" />
				<componentref id="blade_2" />
				<componentref id="blade_3" />
				<componentref id="blade_4" />
				<componentref id="blade_5" />
				<componentref id="blade_6" />
			</children>
		</component>
		<component id="blade">
			<transformation>
				<rotate axis="x" angle="35" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<primitiveref id="bladeRect" />
				<componentref id="blade_back" />
			</children>
		</component>
		<component id="blade_back">
			<transformation>
				<rotate axis="x" angle="180" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<primitiveref id="bladeRect" />
			</children>
		</component>
		<component id="blade_motor">
			<transformation>
				<rotate axis="x" angle="-90"/>
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<primitiveref id="pyramid8" />
			</children>
		</component>
		<component id="blade_1">
			<transformation></transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="blade" />
			</children>
		</component>
		<component id="blade_2">
			<transformation>
				<rotate axis="y" angle="60" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="blade" />
			</children>
		</component>
		<component id="blade_3">
			<transformation>
				<rotate axis="y" angle="120" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="blade" />
			</children>
		</component>
		<component id="blade_4">
			<transformation>
				<rotate axis="y" angle="180" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="blade" />
			</children>
		</component>
		<component id="blade_5">
			<transformation>
				<rotate axis="y" angle="240" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="blade" />
			</children>
		</component>
		<component id="blade_6">
			<transformation>
				<rotate axis="y" angle="300" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="blade" />
			</children>
		</component>
		<component id="blade_suport">
			<transformation>
            </transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="blade_connect" />
				<componentref id="body_connect" />
			</children>
		</component>
		<component id="blade_connect">
			<transformation>
				<translate x="4" y="-1" z="0" />
				<rotate axis="x" angle="-90" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<primitiveref id="cylinder8" />
			</children>
		</component>
		<component id="body_connect">
			<transformation>
				<scale x="1" y="0.8" z="1" />
				<rotate axis="y" angle="90" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<primitiveref id="cylinder3" />
			</children>
		</component>
		<component id= "arm1">
			<transformation></transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="arm" />
			</children>
		</component>
		<component id= "arm2">
			<transformation>
				<rotate axis="y" angle="90" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="arm" />
			</children>
		</component>
		<component id= "arm3">
			<transformation>
				<rotate axis="y" angle="180" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="arm" />
			</children>
		</component>
		<component id= "arm4">
			<transformation>
				<rotate axis="y" angle="270" />
			</transformation>
			<materials >
				<material id="inherit"/>
			</materials>
			<texture id="none"/>
			<children>
				<componentref id="arm" />
			</children>
		</component>
	</components>
</lxs>
